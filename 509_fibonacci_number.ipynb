   "source": [
    "### Recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "4277f1d1",
   "metadata": {},
   "outputs": [],
   "source": [
    "x=8"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "9b1e09c9",
   "metadata": {},
   "outputs": [],
   "source": [
    "def fib(x):\n",
    "    if(x==0): \n",
    "        return 0\n",
    "    if(x==1):\n",
    "        return 1\n",
    "    return fib(x-1)+fib(x-2)"
   ]
  },
